@Article{Androulakis1995,
	author={Androulakis, I. P.
	and Maranas, C. D.
	and Floudas, C. A.},
	title={{$\alpha$BB: A global optimization method for general constrained nonconvex problems}},
	journal={Journal of Global Optimization},
	year={1995},
