@INPROCEEDINGS {10378410,
    author = {A. Karimian and R. Tron},
    booktitle = {2023 IEEE/CVF International Conference on Computer Vision (ICCV)},
    title = {Essential Matrix Estimation using Convex Relaxations in Orthogonal Space},
    year = {2023},
    volume = {},
    issn = {},
    pages = {17096-17106},
    abstract = {We introduce a novel method to estimate the essential matrix for two-view Structure from Motion (SfM). We show that every 3 × 3 essential matrix can be embedded in a 4 × 4 rotation having its bottom right entry fixed to zero; we call the latter the quintessential matrix. This embedding leads to rich relations with the space of 4-D rotations, quaternions, and the classical twisted-pair ambiguity in two-view SfM. We use this structure to derive a succession of semidefinite relaxations that require fewer parameters than the existing non-minimal solvers and yield faster convergence with certifiable optimality. We then exploit the low-rank geometry of these relaxations to reduce them to an equivalent optimization on a Riemannian manifold and solve them via the Riemannian Staircase method. The experimental evaluation confirms that our algorithm always finds the globally optimal solution and outperforms the existing non-minimal methods. We make our implementations open source . 1.},
    keywords = {manifolds;geometry;computer vision;structure from motion;quaternions;estimation;optimization},
    doi = {10.1109/ICCV51070.2023.01572},
    url = {https://doi.ieeecomputersociety.org/10.1109/ICCV51070.2023.01572},
